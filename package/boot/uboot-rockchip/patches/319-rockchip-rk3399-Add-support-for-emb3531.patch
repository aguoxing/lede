+		enable-active-high;
+		gpio = <&gpio2 0x06 0x00>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&vcc_sys_en>;		
+		regulator-name = "vcc_sys";
+		regulator-always-on;
+		regulator-boot-on;
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+	};
+	
